{
	"name": "tomzx/php-semver-checker-git",
	"description": "PHP Composer Template",
	"type": "library",
	"keywords": [
		"semantic versioning",
		"semver",
		"checker"
	],
	"homepage": "https://github.com/tomzx/php-semver-checker-git",
	"license": "MIT",
	"authors": [
		{
			"name": "Tom Rochette",
			"email": "tom@tomrochette.com",
			"homepage": "http://www.tomrochette.com"
		}
	],
	"support": {
		"issues": "https://github.com/tomzx/php-semver-checker-git/issues"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/tomzx/php-parser"
		},
		{
			"type": "vcs",
			"url": "https://github.com/tomzx/gitter"
		}
	],
	"require": {
		"php": ">=5.4.0",

		"herrera-io/phar-update": "~2.0",
		"klaussilveira/gitter": "dev-php-semver-checker",
		"nikic/php-parser": "dev-features/php-semver-checker",
		"symfony/console": "~2.7@dev",
		"tomzx/finder": "~0.1@dev",
		"tomzx/php-semver-checker": "~0.6",
		"vierbergenlars/php-semver": "~3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4"
	},
	"autoload": {
		"psr-4": {
			"PHPSemVerCheckerGit\\": "src/PHPSemVerCheckerGit"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.5-dev"
		}
	}
}
